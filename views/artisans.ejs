<html>
  <!-- Compiled and minified CSS -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0-beta/css/materialize.min.css">
     <title>Protsahan | StateArtisans</title>
    <!-- Compiled and minified JavaScript -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0-beta/js/materialize.min.js"></script>
    <script defer src="https://use.fontawesome.com/releases/v5.0.9/js/all.js" integrity="sha384-8iPTk2s/jMVj81dnzb/iFR2sdA7u06vHJyyLlAd4snFpCl/SnyUjRrbdJsw1pGIl" crossorigin="anonymous"></script>
    <link href="https://fonts.googleapis.com/css?family=Lato:100,300,400,700,900" rel="stylesheet">
  <style>

	body {
	background-color:#F1F1F1;
  font-family: "Lato",sans-serif;
	}

	.nav-wrapper {
	background-color:black;
	height:20px;
	}
  .navbar-fixed nav ul li{
    padding-right: 10px;
  }
  .navbar-fixed nav ul li a:hover{
    background-color: green;
    transition: 0.8s all;
  }
	.card-image {
   height:250px;
	}

	</style>

</head>

<body>

   <div class="navbar-fixed">
    <nav>
      <div class="nav-wrapper" style="height:90px;">
        <a href="#!" class="brand-logo" style="padding-top:10px; padding-left:10px;">Protsahan</a>
        <ul class="right hide-on-med-and-down">
          <li style="padding-top:13px; padding-right:15px;"><a style="font-size:18px;" href="/dashboard" >Home</a></li>
          <li style="padding-top:13px;padding-right:15px;"><a style="font-size:18px;" href="/">Logout</a></li>
        </ul>
      </div>
    </nav>
	</div>

	 <div class="row"style="margin-top:40px;">
	 	<% for(var i=0;i < artisans.length;i++) {  %>
    <div class="col s12 m4">
      <div class="card">
        <div class="card-image">
          <%= console.log(artisans[i].profilephoto.substring(6)) %>
          <img style="height:300px;" src="<%= artisans[i].profilephoto.substring(6) %>">
        </div>
        <div class="card-content">

        </div>
        <div class="card-action">
          <a href="/dashboard/artisan/profile?id=<%=artisans[i]._id%>" style="text-align:center;"><%=artisans[i].name %></a>|&nbsp;&nbsp;<%=artisans[i].skills[0] %>
        </div>
      </div>
    </div>
	<% } %>
  </div>
  <footer class="page-footer" style="background-color:black; height:90px;">

         <div class="footer-copyright">
           <div class="container" style="position:relative;left:32%;font-size:24px;font-weight:700;">
           Â©PROTSAHAN
           </div>
         </div>
       </footer>

</body>
</html>





